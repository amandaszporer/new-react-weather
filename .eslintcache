ric`;\n      axios.get(apiUrl).then(updateWeather);\n      return (\n        <div className=\"Weather\">\n          <Container>\n          {form}\n         </Container>\n        </div>\n      );\n      \n    }\n}\n\nexport default Weather;\n","C:\\Users\\Amanda\\Desktop\\SheCodes\\new-react-weather\\src\\reportWebVitals.js",[],["20","21"],{"ruleId":"22","replacedBy":"23"},{"ruleId":"24","replacedBy":"25"},{"ruleId":"26","severity":1,"message":"27","line":58,"column":70,"nodeType":"28","endLine":58,"endColumn":85},{"ruleId":"22","replacedBy":"29"},{"ruleId":"24","replacedBy":"30"},"no-native-reassign",["31"],"no-negated-in-lhs",["32"],"react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a security risk: see https://html.spec.whatwg.org/multipage/links.html#link-type-noopener","JSXAttribute",["31"],["32"],"no-global-assign","no-unsafe-negation"]